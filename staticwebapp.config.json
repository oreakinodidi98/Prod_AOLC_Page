{
  "routes": [
    {
      "route": "/",
      "serve": "/index.html"
    },
    {
      "route": "/objectives",
      "serve": "/objectives.html"
    },
    {
      "route": "/resources",
      "serve": "/resources.html"
    },
    {
      "route": "/contact",
      "serve": "/contact.html"
    }
  ],
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html"
    },
    "redirect-www": {
      "statusCode": 301,
      "redirect": "https://aolc.live"
    }
  },
  "globalHeaders": {
    "Strict-Transport-Security": "max-age=31536000; includeSubDomains; preload"
  }
}
