{
  "routes": [
    {
      "route": "/",
      "serve": "/index.html"
    },
    {
      "route": "/objectives",
      "serve": "/objectives/index.html"
    },
    {
      "route": "/resources",
      "serve": "/resources/index.html"
    },
    {
      "route": "/contact",
      "serve": "/contact/index.html"
    }
  ],
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html"
    }
  },
  "globalHeaders": {
    "Strict-Transport-Security": "max-age=31536000; includeSubDomains; preload"
  }
}
